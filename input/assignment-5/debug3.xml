<?xml version="1.0"?>

<Molecules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:noNamespaceSchemaLocation="../../src/fileHandling/reader/XMLHandling/ConfigurationFile.xsd">
    <OutputFileName>Debug</OutputFileName>
    <OutputFrequency>1</OutputFrequency>
    <ThermostatConfig>
        <Type>Flow</Type>
        <InitialTemperature>40</InitialTemperature>
        <TargetTemperature>40</TargetTemperature>
        <InitialBrownian>true</InitialBrownian>
        <ScalingGradually>false</ScalingGradually>
        <ApplyAfterHowManySteps>10</ApplyAfterHowManySteps>
        <Dimension>3</Dimension>
    </ThermostatConfig>
    <GravityConfig>
        <X>0</X>
        <Y>-0.8</Y>
        <Z>0</Z>
    </GravityConfig>
    <model>
        <Name>LinkedCells</Name>
        <t_end>500</t_end>
        <delta_t>0.0005</delta_t>
        <force>LeonardJonesForce</force>
        <DomainSize>
            <First>8</First>
            <Second>8</Second>
            <Third>8</Third>
        </DomainSize>
        <rCutOff>2.75</rCutOff>
        <BoundaryCondition>
            <boundaries>
                <Front>Periodic</Front>
                <Back>Periodic</Back>
                <Left>Periodic</Left>
                <Right>Periodic</Right>
                <Top>Periodic</Top>
                <Bottom>Periodic</Bottom>
            </boundaries>
        </BoundaryCondition>
    </model>
    <Cuboids>
        <Size>1</Size>
        <Cuboid>
            <Position>
                <X>3</X>
                <Y>3</Y>
                <Z>0</Z>
            </Position>
            <Velocity>
                <X>0</X>
                <Y>0</Y>
                <Z>0</Z>
            </Velocity>
            <N1>2</N1>
            <N2>2</N2>
            <N3>2</N3>
            <Distance>1.2</Distance>
            <Mass>1</Mass>
            <Brownian>0</Brownian>
            <DimensionBrownian>0</DimensionBrownian>
            <Sigma>1</Sigma>
            <Epsilon>1</Epsilon>
        </Cuboid>
    </Cuboids>
</Molecules>