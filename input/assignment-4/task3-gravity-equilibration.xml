<?xml version="1.0"?>

<Molecules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:noNamespaceSchemaLocation="../../src/fileHandling/reader/XMLHandling/ConfigurationFile.xsd">
    <OutputFileName>Equilibration</OutputFileName>
    <OutputFrequency>100</OutputFrequency>
    <ThermostatConfig>
        <InitialTemperature>0.5</InitialTemperature>
        <TargetTemperature>0.5</TargetTemperature>
        <InitialBrownian>true</InitialBrownian>
        <ScalingGradually>false</ScalingGradually>
        <MaximumTemperatureChange>1</MaximumTemperatureChange>
        <ApplyAfterHowManySteps>1000</ApplyAfterHowManySteps>
        <Dimension>2</Dimension>
    </ThermostatConfig>
    <GravityConfig>
        <GravitationFactor>-12.44</GravitationFactor>
    </GravityConfig>
    <model>
        <Name>LinkedCells</Name>
        <t_end>15</t_end>
        <delta_t>0.0005</delta_t>
        <force>LeonardJonesForce</force>
        <DomainSize>
            <First>303</First>
            <Second>180</Second>
            <Third>0</Third>
        </DomainSize>
        <rCutOff>3</rCutOff>
        <BoundaryCondition>
            <boundaries>
                <Front>Reflective</Front>
                <Back>Reflective</Back>
                <Left>Reflective</Left>
                <Right>Reflective</Right>
                <Top>Reflective</Top>
                <Bottom>Reflective</Bottom>
            </boundaries>
        </BoundaryCondition>
    </model>
    <Cuboids>
        <Size>1</Size>
        <Cuboid>
            <Position>
                <X>1.5</X>
                <Y>2</Y>
                <Z>0</Z>
            </Position>
            <Velocity>
                <X>0</X>
                <Y>0</Y>
                <Z>0</Z>
            </Velocity>
            <N1>250</N1>
            <N2>50</N2>
            <N3>1</N3>
            <Distance>1.2</Distance>
            <Mass>1</Mass>
            <Brownian>0</Brownian>
            <DimensionBrownian>0</DimensionBrownian>
            <Sigma>1.2</Sigma>
            <Epsilon>1</Epsilon>
        </Cuboid>
    </Cuboids>
</Molecules>